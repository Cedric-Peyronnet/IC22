<Window x:Name="addRowGeneralWindow" x:Class="WpfControls.AddRowWindow"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:WpfControls"
      mc:Ignorable="d" d:DesignWidth="300"
      Title="AddRowWindow" Height="100" Width="450">

    <Grid Margin="0,0,0,-219">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="80*"/>
            <ColumnDefinition Width="141*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="okRowAddButton" Content="Ok" HorizontalAlignment="Left" Margin="170,0,0,240" Width="80" Click="okRowAddButton_Click" Grid.Column="1" Height="20" VerticalAlignment="Bottom"/>
        <Button x:Name="cancelRowAddButton" Content="Cancel" HorizontalAlignment="Left" Margin="52,0,0,240" Width="80" Click="cancelRowAddButton_Click" Grid.Column="1" Height="20" VerticalAlignment="Bottom"/>
        <Grid x:Name="myLabelsGrid" HorizontalAlignment="Left" Width="436" VerticalAlignment="Top" UseLayoutRounding="True" RenderTransformOrigin="0,0" Height="707" Grid.ColumnSpan="2" Margin="0,0,0,-201"/>
    </Grid>
</Window>
