<Window x:Class="WpfControls.EditCheck"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfControls"
        mc:Ignorable="d"
        Title="editCheck" Height="120" Width="220.5">
    <Grid>
        <Button x:Name="Cancel" Content="Cancel" HorizontalAlignment="Left" Margin="10,55,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
        <Button x:Name="Ok" Content="Ok" HorizontalAlignment="Left" Margin="127,55,0,0" VerticalAlignment="Top" Width="75" Click="Ok_Click"/>
        <Label x:Name="CheckPopupLabel" Content="setLabelText" HorizontalAlignment="Left" Margin="10,7,0,0" VerticalAlignment="Top" Width="192" Height="38"/>
    </Grid>
</Window>
